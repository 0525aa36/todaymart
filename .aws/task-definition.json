{
  "family": "korean-agri-shop-backend",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::940482431364:role/korean-agri-shop-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::940482431364:role/korean-agri-shop-ecs-task-role",
  "cpu": "512",
  "memory": "1024",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "940482431364.dkr.ecr.ap-northeast-2.amazonaws.com/korean-agri-shop/backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "8081"
        },
        {
          "name": "CORS_ALLOWED_ORIGINS",
          "value": "https://todaymart.co.kr,https://www.todaymart.co.kr,https://main.d3o2xbkegs5wzg.amplifyapp.com,http://localhost:3000,https://api.todaymart.co.kr"
        },
        {
          "name": "AWS_REGION",
          "value": "ap-northeast-2"
        },
        {
          "name": "DB_USERNAME",
          "value": "admin"
        },
        {
          "name": "DB_HOST",
          "value": "korean-agri-shop-mysql.clm60gwkayez.ap-northeast-2.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_NAME",
          "value": "agrimarket"
        },
        {
          "name": "FILE_STORAGE_TYPE",
          "value": "s3"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "korean-agri-shop-uploads"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "FRONTEND_URL",
          "value": "https://todaymart.co.kr"
        },
        {
          "name": "BACKEND_URL",
          "value": "https://api.todaymart.co.kr"
        },
        {
          "name": "NAVER_CLIENT_ID",
          "value": "mclMxOndgnb3NwKPSLcx"
        },
        {
          "name": "KAKAO_CLIENT_ID",
          "value": "56380206602abece46af0dabc59a7c66"
        },
        {
          "name": "TOSS_PAYMENTS_CLIENT_KEY",
          "value": "test_ck_yZqmkKeP8gBK9y9X4lGOrbQRxB9l"
        },
        {
          "name": "GOOGLE_SHEETS_ENABLED",
          "value": "true"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/rds/credentials-Ja2RDJ:password::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/oauth2-SjPSuq:JWT_SECRET::"
        },
        {
          "name": "NAVER_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/oauth2-SjPSuq:NAVER_CLIENT_SECRET::"
        },
        {
          "name": "KAKAO_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/oauth2-SjPSuq:KAKAO_CLIENT_SECRET::"
        },
        {
          "name": "TOSS_PAYMENTS_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/payments-a9slXb:TOSS_PAYMENTS_SECRET_KEY::"
        },
        {
          "name": "WEBHOOK_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/payments-a9slXb:WEBHOOK_SECRET::"
        },
        {
          "name": "GOOGLE_SHEETS_CREDENTIALS",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:940482431364:secret:korean-agri-shop/google-sheets-79aKmT"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/korean-agri-shop/backend",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8081/actuator/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
